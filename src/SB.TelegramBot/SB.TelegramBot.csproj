<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <RepositoryUrl>https://github.com/Bunyod545/SBTelegramBot</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <Version>1.0.72</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/Bunyod545/SBTelegramBot</PackageProjectUrl>
    <AssemblyVersion>1.0.60.0</AssemblyVersion>
    <Authors>Bunyod545</Authors>
    <Company>SB</Company>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Logics\TelegramBotDIContainers\RegisterServices\**" />
    <EmbeddedResource Remove="Logics\TelegramBotDIContainers\RegisterServices\**" />
    <None Remove="Logics\TelegramBotDIContainers\RegisterServices\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="5.2.0" />
    <PackageReference Include="LiteDB" Version="5.0.12" />
    <PackageReference Include="Telegram.Bot" Version="18.0.0" />
  </ItemGroup>

</Project>
